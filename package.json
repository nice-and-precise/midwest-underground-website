{
  "name": "midwest-underground-website",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:sandbox": "srt 'npm run dev'",
    "build": "next build",
    "build:sandbox": "srt 'npm run build'",
    "start": "next start",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:seed": "ts-node --project tsconfig.seed.json prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "test": "vitest run",
    "test:sandbox": "srt --settings .claude/agent-configs/agent-9-testing.json 'npm test'",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:sandbox": "srt --settings .claude/agent-configs/agent-9-testing.json 'npm run test:e2e'",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:takeoff": "playwright test tests/takeoff",
    "test:e2e:update-snapshots": "playwright test --update-snapshots",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test && npm run test:e2e",
    "agent:9": "srt --settings .claude/agent-configs/agent-9-testing.json 'npm run dev'",
    "agent:10": "srt --settings .claude/agent-configs/agent-10-content.json 'npm run dev'",
    "agent:11": "srt --settings .claude/agent-configs/agent-11-seo.json 'npm run dev'",
    "agent:12": "srt --settings .claude/agent-configs/agent-12-email.json 'npm run dev'",
    "agent:13": "srt --settings .claude/agent-configs/agent-13-deployment.json 'npm run dev'",
    "agent:14": "srt --settings .claude/agent-configs/agent-14-performance.json 'npm run dev'",
    "agent:15": "srt --settings .claude/agent-configs/agent-15-security.json 'npm run dev'",
    "agent:16": "srt --settings .claude/agent-configs/agent-16-mobile.json 'npm run dev'",
    "agent:17": "srt --settings .claude/agent-configs/agent-17-docs.json 'npm run dev'",
    "docs:audit": "bash scripts/docs/audit-docs.sh",
    "docs:validate": "node scripts/docs/validate-links.js",
    "docs:check-colors": "bash scripts/docs/check-brand-colors.sh",
    "docs:generate-toc": "node scripts/docs/generate-toc.js",
    "docs:check-all": "npm run docs:validate && npm run docs:check-colors"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "bcryptjs": "^3.0.3",
    "chart.js": "^4.4.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.555.0",
    "next": "^15.5.6",
    "next-auth": "^5.0.0-beta.29",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/leaflet": "^1.9.14",
    "@types/node": "^24.10.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.22",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "msw": "^2.12.3",
    "postcss": "^8.5.6",
    "prisma": "^6.0.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^4.0.13"
  }
}
